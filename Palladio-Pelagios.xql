let $col := collection('epidoc/?select=*.xml')
return
          for $rs in $col//t:rs
          let $root := $rs/ancestor::t:TEI
          let $date := string($root//t:origDate/@notBefore) || '; '|| string($root//t:origDate/@notAfter) || '; '|| string($root//t:origDate/@when)
          let $period := replace($root//t:origDate/@period, '#', '')
          let $periodAuth := doc('../webapps/ROOT/content/xml/authority/per.xml')//t:item[@xml:id = $period]/t:*[@xml:lang='it']/text()
          let $placeid := replace($rs/@ref, '#', '')
          let $placeAuth := doc('../webapps/ROOT/content/xml/authority/pl.xml')//t:place[@xml:id = $placeid]
          let $placeName := $placeAuth//t:placeName[@xml:lang = 'it']/text()
          let $pleiades:= $placeAuth//t:idno[@type='pleiades']/text()
          let $pleiadescoord := $placescoords//place[id=$pleiades]/coord/text()
          let $monumentid := replace($root//t:objectType/@ref, '#', '')
          let $monumentAuth := doc('../webapps/ROOT/content/xml/authority/mon.xml')//t:item[@xml:id = $monumentid]
          let $monumentName := $monumentAuth/t:*[@xml:lang='it']/text()
          let $monumentId := $monumentAuth/t:ref/text()
          let $documentid := replace($root//t:summary/@corresp, '#', '')
          let $documentAuth := doc('../webapps/ROOT/content/xml/authority/doc.xml')//t:item[@xml:id = $documentid]
          let $documentName := $documentAuth/t:*[@xml:lang='it']/text()
          let $documentId := $documentAuth/t:ref/text()
          let $placeType := replace($placeAuth//t:state/@key, '#', '')
          
          (: PELAGIOS - START :)
          for $idno in $col//t:idno[@type='projectNo']/text()
          let $root := $idno/ancestor::t:TEI
          let $docno := concat('https://ilc4clarin.ilc.cnr.it/cretaninscriptions/en/inscriptions/', doc('all_inscriptions.xml')//t:item[@sortKey=$idno]/@n, '.html')
          let $title := $root//t:titleStmt/t:title
          let $monumentid := replace($root//t:objectType/@ref, '#', '')
          let $mon := doc('mon.xml')//t:item[@xml:id = $monumentid]/t:*[@xml:lang='en']/text()
          let $documentid := replace($root//t:summary/@corresp, '#', '')
          let $doc := doc('doc.xml')//t:item[@xml:id = $documentid]/t:*[@xml:lang='en']/text()
          let $date := $root//t:origDate/@notBefore || '/' || $root//t:origDate/@notAfter || ' ' || $root//t:origDate/@when
          let $origplace := replace($root//t:origPlace/@ref, '#', '')          
          let $origpleiades := concat('https://pleiades.stoa.org/places/', doc('pl.xml')//t:place[@xml:id = $origplace]//t:idno[@type='pleiades']/text())
          let $mentionedplace := distinct-values($root//t:div[@type='edition']//t:placeName/@ref )
          let $mentionedpleiades := for $v in $mentionedplace return concat('https://pleiades.stoa.org/places/', doc('pl.xml')//t:place[concat('#', @xml:id) = $v]//t:idno[@type='pleiades']/text(), ' ')
          (: PELAGIOS - END :)
          
           return
           (string($rs/@subtype), ';', string($rs/@key), ';', string($rs/@role), ';'
           , $placeName, ';', $placeType, ';', $pleiades, ';', $pleiadescoord, ';', normalize-space(replace(string-join($rs//text()), ',', '')), ';', 
           replace($root//t:idno[@type='filename']/text(), ',', ' '), ';', replace($monumentName, ',', ' '), ';', $monumentId, ';', 
           replace($documentName, ',', ' '), ';',  $documentId, ';', $date, ';', $periodAuth, '
') (:remove spaces from: docno, when, mentionedpleiades, origpleiades; add [ at beginning and ] at end, removing last comma; multiple places?:)

