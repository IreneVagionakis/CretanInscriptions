                        <ab>
                          <lb n="0"/><rs type="institution" subtype="board" role="builder" key="naokoros" ref="#lebena"><persName type="attested" key="- f. Antiphatas" cert="low"><gap reason="lost" quantity="1" unit="line"/>
                          <lb n="1"/><name nymRef="Ἀντιφάτας" type="patronymic">Ἀντιφ<unclear>ά</unclear><supplied reason="lost" cert="low">τα</supplied></name></persName></rs>,
                          <lb n="2"/><rs type="institution" subtype="board" role="builder" key="naokoros" ref="#lebena"><persName type="attested" key="Kartaidamas f. Mostylion"><name nymRef="Καρταιδάμας">Καρταιδ<unclear>ά</unclear><supplied reason="lost">μας</supplied></name>
                          <lb n="3"/><name nymRef="Μοστυλίων" type="patronymic">Μοστυλίων<supplied reason="lost">ος</supplied></name></persName>
                          <lb n="4"/><del rend="erasure"><orig>μα</orig></del> <w lemma="νακορέω">νακορήσαν<lb 
                        break="no" n="5"/>τες</w></rs> <w lemma="τίθημι">ἔθηκαν</w>
                          <lb n="6"/>τὸν <rs type="institution" subtype="public_space" role="construction" key="choros" ref="#lebena"><w lemma="χορός">χορὸν</w></rs>
                          <lb n="7"/>ἀπὸ τῶ <rs type="institution" subtype="public_space" role="construction" key="adyton" ref="#lebena"><w lemma="ἄδυτον">ἀδύτω</w></rs>.
                          </ab>
