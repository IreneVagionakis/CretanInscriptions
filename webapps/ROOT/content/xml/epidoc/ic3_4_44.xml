                        <ab>
                          <lb n="1"/><rs type="institution" subtype="official" role="eponym" key="kosmos" ref="#gortyn" cert="low">ἐπὶ <persName type="attested" key="Pa- f. -"><name nymRef="Πα-">Πα<gap reason="lost" extent="unknown" unit="character"/></name> <supplied reason="lost">τῶ</supplied> <gap reason="lost" extent="unknown" unit="character"/></persName></rs> <supplied reason="lost">οἱ</supplied> <rs type="institution" subtype="board" role="dedicant" key="agoranomos" ref="#gortyn"><w lemma="ἀγορανόμος"><supplied reason="lost">ἀγορα</supplied><lb 
                        break="no" n="2"/>νόμοι</w></rs> <gap reason="lost" extent="unknown" unit="character"/>
                          <lb n="3"/>τίδα Τ<gap reason="lost" extent="unknown" unit="character"/>
                          <lb n="4"/>Σατ<gap reason="lost" extent="unknown" unit="character"/>
                          <lb n="5"/>Σατ<gap reason="lost" extent="unknown" unit="character"/>
