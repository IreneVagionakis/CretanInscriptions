                    <div type="edition" xml:lang="grc" xml:space="preserve">
                        <ab>
                          <lb n="1"/><w lemma="θιός">θιοί</w>. ἀγαθᾶ<supplied reason="lost">ι</supplied> <w lemma="τύχα"><supplied reason="lost">τύ</supplied>χαι</w>.
                          <lb n="2"/><rs type="institution" subtype="tribe" role="eponym" key="Hylleis" ref="#lato">ἐπὶ τῶν <w lemma="Ὑλλεῖς">Ὑλλέων</w></rs> <rs type="institution" subtype="board" role="eponym" key="kosmos" ref="#lato"><w lemma="κοσμέω">κοσμιόντων</w> τῶν σὺν
                          <lb n="3"/><persName type="attested" key="Kydannos f. Enipas"><name nymRef="Κύδαννος">Κυδάννωι</name> τῶ <name nymRef="Ἐνίπας" type="patronymic">Ἐνίπαντος</name></persName></rs>, <rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Bergis f. Euagoros"><name nymRef="Βέργις">Βέργιος</name> τῶ <name nymRef="Εὐάγορος" type="patronymic">Εὐαγόρω</name></persName></rs>,
                          <lb n="4"/><rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Lattygos f. Damochares"><name nymRef="Λάττυγος">Λαττύγω</name> τῶ <name nymRef="Δαμοχάρης" type="patronymic">Δαμοχάριος</name></persName></rs>, <rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Porthesilas f. Antipatas"><name nymRef="Πορθεσίλας">Πορθεσίλα</name> τῶ <name nymRef="Ἀντιπάτας" type="patronymic">Ἀντιπάτα</name></persName></rs>,
                          <lb n="5"/><rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Pheidon f. Dallos"><name nymRef="Φείδων">Φείδωνος</name> τῶ <name nymRef="Δάλλος" type="patronymic">Δάλλω</name></persName></rs>, <rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Porthesilas f. Klesippos"><name nymRef="Πορθεσίλας">Πορθεσίλα</name> τῶ <name nymRef="Κλήσιππος" type="patronymic">Κλησίππω</name></persName></rs>,
                          <lb n="6"/><rs type="institution" subtype="board" role="incumbent" key="kosmos" ref="#lato"><persName type="attested" key="Kletonymos f. Mnastokles"><name nymRef="Κλητώνυμος">Κλητωνύμω</name> τῶ <name nymRef="Μναστοκλῆς" type="patronymic">Μναστοκλεῖος</name></persName></rs>, <rs type="institution" subtype="official" role="incumbent" key="grammateus" ref="#lato"><w lemma="γραμματεύς">γραμματέος</w>
                          <lb n="7"/><persName type="attested" key="Mnaston f. Mnastokles"><name nymRef="Μνάστων">Μνάστωνος</name> τῶ <name nymRef="Μναστοκλῆς" type="patronymic">Μναστοκλεῖος</name></persName></rs> <space extent="unknown" unit="character"/> ἁ <rs type="institution" subtype="assembly" role="builder" key="polis" ref="#lato"><w lemma="πόλις">πόλις</w></rs>
