                        <ab>
                          <lb n="1"/><supplied reason="lost">ἀ</supplied>γαθᾶι <w lemma="τύχα">τύχαι</w>.
                          <lb n="2"/><rs type="institution" subtype="tribe" role="eponym" key="Echanoreis" ref="#lato">ἐπὶ τῶν <w lemma="Ἐχανορεῖς">Ἐχανορέων</w></rs> <rs type="institution" subtype="board" role="eponym" key="kosmos" ref="#lato"><w lemma="κοσμέω">κοσμιόντω<supplied reason="lost">ν</supplied></w></rs>
